local NotificationHolder = loadstring(game:HttpGet("https://raw.githubusercontent.com/BocusLuke/UI/main/STX/Module.Lua"))()
local Notification = loadstring(game:HttpGet("https://raw.githubusercontent.com/BocusLuke/UI/main/STX/Client.Lua"))()
wait(0.7)

local ScreenGui = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")
local TitleLabel = Instance.new("TextLabel")
local Button = Instance.new("TextButton")
local LoadingLabel = Instance.new("TextLabel")

ScreenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
Frame.Parent = ScreenGui
Frame.Draggable = true
Frame.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
Frame.Position = UDim2.new(0.5, -150, 0.5, -100)
Frame.Size = UDim2.new(0, 250, 0, 125)
Frame.BorderSizePixel = 0
Frame.BackgroundTransparency = 0.2

local UICorner = Instance.new("UICorner")
UICorner.Parent = Frame
UICorner.CornerRadius = UDim.new(0, 12)

TitleLabel.Parent = Frame
TitleLabel.BackgroundTransparency = 1
TitleLabel.Position = UDim2.new(0.5, -75, 0, 10)
TitleLabel.Size = UDim2.new(0, 150, 0, 30)
TitleLabel.Text = "Karved Loader"
TitleLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
TitleLabel.Font = Enum.Font.GothamBold
TitleLabel.TextSize = 20
TitleLabel.TextStrokeTransparency = 0.5

Button.Parent = Frame
Button.Font = Enum.Font.GothamBold
Button.BackgroundColor3 = Color3.fromRGB(0, 170, 0)
Button.Position = UDim2.new(0.5, -50, 0.5, -20)
Button.Size = UDim2.new(0, 100, 0, 35)
Button.TextSize = 18
Button.TextColor3 = Color3.fromRGB(255, 255, 255)
Button.Text = "Load Script"
Button.AutoButtonColor = false

Button.MouseEnter:Connect(function()
    Button.BackgroundColor3 = Color3.fromRGB(0, 200, 0)
end)

Button.MouseLeave:Connect(function()
    Button.BackgroundColor3 = Color3.fromRGB(0, 170, 0)
end)

LoadingLabel.Parent = Frame
LoadingLabel.BackgroundTransparency = 0.5
LoadingLabel.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
LoadingLabel.Position = UDim2.new(0.5, -50, 0.5, 20)
LoadingLabel.Size = UDim2.new(0, 100, 0, 20)
LoadingLabel.Text = "V1.01"
LoadingLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
LoadingLabel.Font = Enum.Font.Gotham
LoadingLabel.TextSize = 14

local canClick = true

local function loading()
    if not canClick then
    Button.BackgroundColor3 = Color3.fromRGB(0, 170, 0) wait(5) return end
    canClick = false
    local loadingTexts = {"Loading", "Loading.", "Loading..", "Loading..."}
    for i = 1, 3 do
        LoadingLabel.Text = loadingTexts[i % #loadingTexts + 1]
        wait(math.random(1, 1.2))
    end

    loadstring(game:HttpGet("https://raw.githubusercontent.com/Hl3n/karvxhub/refs/heads/main/zcxx", true))()
    Notification:Notify(
        {Title = ".gg/karved", Description = "Script Loaded!"},
        {OutlineColor = Color3.fromRGB(80, 80, 80), Time = 5, Type = "default"}
    )
    
    wait(0.1)
    LoadingLabel.Text = "Script Loaded!"
    wait(1)
    LoadingLabel.Text = "Closing Loader."
    wait(0.6)
    ScreenGui:Destroy()
    wait(5)
    canClick = true
    Button.BackgroundColor3 = Color3.fromRGB(0, 170, 0)
    end


Button.MouseButton1Click:Connect(loading)
